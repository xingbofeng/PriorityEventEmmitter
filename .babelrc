{
    "presets": [
        ["env", {
            "targets": {
                "browsers": [
                    "last 3 Chrome versions",
                    "last 3 Firefox versions",
                    "Safari >= 10",
                    "Explorer >= 11",
                    "Edge >= 12",
                    "iOS >= 10",
                    "Android >= 6"
                ],
                "node": "6.0",
                
            },
            "modules": "commonjs",
            "debug": false,
            "include": ["transform-es2015-arrow-functions"],
            "useBuiltIns": false
        }],
        "minify"
    ],
}